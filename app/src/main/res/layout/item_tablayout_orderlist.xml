<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/tv_title_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="我的行程"
        android:textColor="#061925"
        android:textSize="@dimen/dimen_14x"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/iv_title_line"
        android:layout_width="@dimen/dimen_14x"
        android:layout_height="@dimen/dimen_4x"
        android:layout_marginTop="@dimen/dimen_8x"
        android:src="@drawable/ge_tab_line"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="@+id/tv_title_name"
        app:layout_constraintStart_toStartOf="@+id/tv_title_name"
        app:layout_constraintTop_toBottomOf="@+id/tv_title_name" />


</androidx.constraintlayout.widget.ConstraintLayout>