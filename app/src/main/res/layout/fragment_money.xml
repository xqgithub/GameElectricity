                <ImageView
                    android:id="@+id/iv_digital"
                    android:layout_width="@dimen/dimen_170x"
                    android:layout_height="@dimen/dimen_170x"
                    android:layout_marginTop="@dimen/dimen_18x"
                    android:src="@drawable/ge_money_digital"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />


