                app:layout_constraintStart_toEndOf="@+id/tv_my_gold_num"
                app:layout_constraintTop_toTopOf="@+id/tv_my_gold_num" />


        </androidx.constraintlayout.widget.ConstraintLayout>

        <!-- 排行榜信息 -->
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/clLeaderboard"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dimen_5x"
            android:layout_marginTop="@dimen/dimen_18x"
            app:layout_constraintStart_toStartOf="parent"
