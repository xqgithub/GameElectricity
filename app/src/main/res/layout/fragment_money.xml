                    android:background="@drawable/ge_money_level_bg"
                    android:paddingHorizontal="@dimen/dimen_6x"
                    android:text="Lv.1"
                    android:textColor="#233556"
                    android:textSize="@dimen/dimen_12x"
                    app:layout_constraintBottom_toBottomOf="@+id/tv_usernickname2"
                    app:layout_constraintStart_toEndOf="@+id/tv_usernickname2"
                    app:layout_constraintTop_toTopOf="@+id/tv_usernickname2" />

                <TextView
                    android:id="@+id/tv_placeholder3"
                    android:layout_width="@dimen/dimen_25x"
                    android:layout_height="wrap_content"
                    android:paddingHorizontal="@dimen/dimen_6x"
                    app:layout_constraintBottom_toBottomOf="@+id/tv_usernickname2"
                    app:layout_constraintStart_toEndOf="@+id/tv_level2"
                    app:layout_constraintTop_toTopOf="@+id/tv_usernickname2" />


                <ImageView
                    android:id="@+id/iv_my_gold2"
