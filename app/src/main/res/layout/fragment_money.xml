                android:layout_marginTop="@dimen/dimen_5x"
                android:ellipsize="end"
                android:maxLines="1"
                android:text="玩家昵称最多玩家昵称最多"
                android:textColor="#233556"
                android:textSize="@dimen/dimen_12x"
                app:layout_constraintStart_toEndOf="@+id/siv_avatar"
                app:layout_constraintTop_toTopOf="@+id/siv_avatar" />

            <TextView
                android:id="@+id/tv_level"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
