                    app:layout_constraintStart_toStartOf="@+id/tv_usernickname2"
                    app:layout_constraintTop_toBottomOf="@+id/tv_usernickname2" />

                <TextView
                    android:id="@+id/tv_my_gold_num2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dimen_4x"
                    android:maxLength="5"
                    android:maxLines="1"
                    android:text="9999k"
                    android:textColor="#233556"
                    android:textSize="@dimen/dimen_18x"
                    app:layout_constraintBottom_toBottomOf="@+id/iv_my_gold2"
