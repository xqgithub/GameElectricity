                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/iv_goose"
                android:layout_width="@dimen/dimen_125x"
                android:layout_height="@dimen/dimen_125x"
                android:layout_marginStart="@dimen/dimen_15x"
                android:layout_marginTop="@dimen/dimen_40x"
                android:src="@drawable/ge_money_goose"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/cl_makeups" />


            <com.sn.gameelectricity.app.weight.customview.ImageAndTextUi3
                android:id="@+id/itui_feeding"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/dimen_120x"
                android:visibility="gone"
                app:layout_constraintBottom_toBottomOf="@+id/iv_goose"
                app:layout_constraintEnd_toEndOf="@+id/iv_goose"
                app:layout_constraintStart_toStartOf="@+id/iv_goose"
                tools:visibility="visible" />

            <RelativeLayout
                android:id="@+id/rl_pag_finger"
                android:layout_width="@dimen/dimen_44x"
                android:layout_height="@dimen/dimen_44x"
                android:layout_marginBottom="@dimen/dimen_100x"
                android:visibility="gone"
                app:layout_constraintBottom_toBottomOf="@+id/iv_goose"
                app:layout_constraintEnd_toEndOf="@+id/iv_goose" />


        </androidx.constraintlayout.widget.ConstraintLayout>

