                    android:paddingHorizontal="@dimen/dimen_8x"
                    android:paddingVertical="@dimen/dimen_2x"
                    android:text="去兑换"
                    android:textColor="#FFFFFF"
                    android:textSize="@dimen/dimen_12x"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@+id/tv_my_gold_num2"
                    app:layout_constraintStart_toEndOf="@+id/tv_my_gold_num2"
                    app:layout_constraintTop_toTopOf="@+id/tv_my_gold_num2" />


                <ImageView
                    android:id="@+id/iv_dress2"
                    android:layout_width="@dimen/dimen_44x"
                    android:layout_height="@dimen/dimen_48x"
                    android:layout_marginTop="@dimen/dimen_25x"
                    android:layout_marginEnd="@dimen/dimen_20x"
                    android:src="@drawable/ge_money_dress"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/siv_avatar2" />


                <ImageView
                    android:id="@+id/iv_paradise2"
                    android:layout_width="@dimen/dimen_44x"
                    android:layout_height="@dimen/dimen_48x"
                    android:layout_marginTop="@dimen/dimen_8x"
                    android:src="@drawable/ge_money_paradise"
                    app:layout_constraintEnd_toEndOf="@+id/iv_dress2"
                    app:layout_constraintTop_toBottomOf="@+id/iv_dress2" />


                <ImageView
                    android:id="@+id/iv_dynamic2"
                    android:layout_width="@dimen/dimen_44x"
                    android:layout_height="@dimen/dimen_48x"
                    android:layout_marginTop="@dimen/dimen_8x"
                    android:src="@drawable/ge_money_dynamic"
                    app:layout_constraintEnd_toEndOf="@+id/iv_dress2"
                    app:layout_constraintTop_toBottomOf="@+id/iv_paradise2" />


                <ImageView
                    android:id="@+id/iv_friend2"
                    android:layout_width="@dimen/dimen_44x"
                    android:layout_height="@dimen/dimen_48x"
                    android:layout_marginTop="@dimen/dimen_8x"
                    android:src="@drawable/ge_money_friend"
                    app:layout_constraintEnd_toEndOf="@+id/iv_dress2"
                    app:layout_constraintTop_toBottomOf="@+id/iv_dynamic2" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/cl_mask_layer"
                android:layout_width="match_parent"
                android:layout_height="0dp"
