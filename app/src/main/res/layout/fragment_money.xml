                    app:layout_constraintStart_toEndOf="@+id/siv_avatar2"
                    app:layout_constraintTop_toTopOf="@+id/siv_avatar2" />

                <TextView
                    android:id="@+id/tv_level2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
