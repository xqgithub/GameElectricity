<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true">
        <shape>
            <corners android:radius="34.48dp" />
            <gradient android:angle="0" android:endColor="#ED7A57" android:startColor="#F19B3F" android:type="linear" />
        </shape>
    </item>

    <item android:state_enabled="false">
        <shape>
            <gradient android:angle="0" android:endColor="#66ED7A57" android:startColor="#66F19B3F" android:type="linear" />
            <corners android:radius="34.48dp" />
        </shape>
    </item>
</selector>